{
    "name": "assistant-in-a-box",
    "version": "1.2.0",
    "description": "A sales-driven, context-aware chatbot engine for landing pages.",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "DOCUMENTATION.md"
    ],
    "scripts": {
        "build": "tsup index.ts --format cjs,esm,iife --dts --clean --minify --global-name Assistant",
        "dev": "tsup index.ts --format cjs,esm --watch --dts",
        "test": "vitest run"
    },
    "keywords": [
        "chatbot",
        "sales",
        "search",
        "ai",
        "context-aware",
        "conversion",
        "landing-page"
    ],
    "author": "X.ASKA",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^25.2.0",
        "tsup": "^8.5.1",
        "typescript": "^5.0.0",
        "vitest": "^4.0.18"
    },
    "dependencies": {
        "sastrawijs": "^1.1.0"
    }
}